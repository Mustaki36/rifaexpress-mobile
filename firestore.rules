rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are permissive and are intended for initial development and testing.
    // They allow any authenticated user to read and write all data.
    // You should implement more specific security rules before launching to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}